                        android:textSize="@dimen/txt_size_18" />

                    <com.example.myapplication.view.MytextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="累计采购金额"
                        android:textColor="@color/color_666666"
                        android:textSize="@dimen/txt_size_11" />

                </LinearLayout>

                <View
                    android:layout_width="1dp"
                    android:layout_height="31dp"
                    android:layout_gravity="center"
                    android:background="@color/color_e6e6e6" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    >
<!--                    app:clickview="@{(view)->click.showCashListActivity(view,2,data3.paid_yesterday.income)}"-->

                    <com.example.myapplication.view.MytextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text='@{dataall==null||dataall.economizePrice==null?"¥0.00":"¥"+dataall.economizePrice,default="¥0.00"}'
                        android:textColor="@color/color_333333"
