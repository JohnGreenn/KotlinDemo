<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="fragment"
            type="com.hpkj.sheplive.fragment.MyFragment" />

        <import type="android.view.View" />

        <variable
            name="click"
            type="com.hpkj.sheplive.utils.ClickUtil" />

        <variable
            name="dataall"
            type="com.hpkj.sheplive.entity.UserBean" />

        <variable
            name="data"
            type="com.hpkj.sheplive.entity.LoginBean" />

        <variable
            name="data1"
            type="com.hpkj.sheplive.entity.UserBean.TodayIncomeBean" />

        <variable
            name="data2"
            type="com.hpkj.sheplive.entity.UserBean.FansBean" />


        <variable
            name="data3"
            type="com.hpkj.sheplive.entity.UserBean.IncomesBean" />

        <variable
            name="his"
